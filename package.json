{
  "name": "lumus",
  "author": "František Žiačik <ziacik@gmail.com>",
  "version": "0.4.17",
  "description": "Search for movies, shows and music and add them automatically to Transmission when they appear on torrents. Lightweight all-in-one alternative to SickBeard, CouchPotato and Headphones.",
  "keywords": [
    "lumus",
    "torrent",
    "search",
    "movies",
    "shows",
    "music"
  ],
  "homepage": "http://getlumus.com",
  "preferGlobal": true,
  "bin": {
    "lumus": "./bin/lumus"
  },
  "dependencies": {
    "cheerio": "*",
    "express": "3.4.x",
    "jade": "*",
    "kat-api": "0.0.3",
    "less-middleware": "*",
    "mkdirp": "*",
    "moment": "2.6.x",
    "nconf": "*",
    "nedb": "*",
    "node-tvdb": "~0.4.2",
    "npm": "~2.7.1",
    "opensubtitles-client": "^2.2.0",
    "q": "~1.1.2",
    "request": "^2.51.0",
    "rimraf": "*",
    "thepiratebay": "^0.2.2",
    "transmission": "~0.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ziacik/lumus.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "http://github.com/ziacik/lumus/issues"
  },
  "devDependencies": {
    "chai": "^2.2.0",
    "chai-as-promised": "^4.3.0",
    "mocha": "^2.2.1",
    "proxyquire": "^1.4.0",
    "sinon": "^1.14.1",
    "sinon-chai": "^2.7.0"
  },
  "scripts": {
    "test": "mocha",
    "start": "node app.js"
  }
}
